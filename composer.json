{
    "name": "jefterrafael/cryptedauth",
    "description": "",
    "keywords": ["jefter-rafael"],
    "license": "MIT",
    "authors": [
        {
            "name": "jefter rafael",
            "email": "jefterrafael1.1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0",
        "mariuzzo/laravel-js-localization":"^1.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "jefterrafael\\cryptedauth\\": "src/",
            "Illuminate\\Foundation\\Auth\\": "auth-backend/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jefterrafael\\cryptedauth\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "jefterrafael\\cryptedauth\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
